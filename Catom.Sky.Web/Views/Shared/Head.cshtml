@{
    ViewBag.Title = "天空网";
}

<title >@ViewBag.Title</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link rel="shortcut icon" href="/Content/img/logo/sky.ico" />

@Styles.Render("~/Content/css/bootstrap")
@Styles.Render("~/Content/css/app")
@Styles.Render("~/Content/css/global")

<!-- bootstrap -->
<link href="/Content/css/bootstrap/bootstrap.css" rel="stylesheet" />
<link href="/Content/css/bootstrap/bootstrap-responsive.css" rel="stylesheet" />
<link href="/Content/css/bootstrap/bootstrap-overrides.css" type="text/css" rel="stylesheet" />

<!-- libraries -->
<link href="/Content/css/lib/jquery-ui-1.10.2.custom.css" rel="stylesheet" type="text/css" />
<link href="/Content/css/lib/font-awesome.css" type="text/css" rel="stylesheet" />
<link href="/Content/css/lib/morris.css" type="text/css" rel="stylesheet" />
<link href="/Content/css/lib/bootstrap-wysihtml5.css" type="text/css" rel="stylesheet" />
<link href="/Content/css/lib/bootstrap.datepicker.css" type="text/css" rel="stylesheet" />
<link href="/Content/css/lib/uniform.default.css" type="text/css" rel="stylesheet" />
<link href="/Content/css/lib/select2.css" type="text/css" rel="stylesheet" />

<!-- global styles -->
<link rel="stylesheet" type="text/css" href="/Content/css/global/layout.css" />
<link rel="stylesheet" type="text/css" href="/Content/css/global/elements.css" />
<link rel="stylesheet" type="text/css" href="/Content/css/global/icons.css" />

<!-- open sans font -->
<link href='http://fonts.useso.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800' rel='stylesheet' type='text/css' />

<!-- lato font -->
