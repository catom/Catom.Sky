    <!--oss参数-->
    <add key="ossHost" value="schoolpal.oss-cn-hangzhou.aliyuncs.com" />
    <add key="ossBaseUrl" value="http://oss.aliyuncs.com" />
    <add key="bucketName" value="schoolpal" />
    <add key="accessId" value="gNWOZA84CZCF5BxN" />
